[project]
	description = You can use it for training purpose
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/sysintelligent
[access "refs/for/refs/*"]
	read = group user/sysintelligent
	push = group user/sysintelligent
	label-Code-Review = -2..+2 group user/sysintelligent
	label-Verified = -1..+1 group user/sysintelligent
	rebase = group user/sysintelligent
	removeReviewer = group user/sysintelligent
	submit = group user/sysintelligent
[access "refs/heads/*"]
	read = group user/sysintelligent
	create = group user/sysintelligent
	push = +force group user/sysintelligent
	pushMerge = group user/sysintelligent
[access "refs/tags/*"]
	push = +force group user/sysintelligent
	pushTag = group user/sysintelligent
	pushSignedTag = group user/sysintelligent
